<?xml version="1.0" encoding="UTF-8"?>
<service name="GetAddresses" version="1">
    <request>
        <parameter name="customerNo"/>
    </request>
    <actions>
        <persist-request-action>
            <output-parameter name="request_id"/>
        </persist-request-action>
        <db-action sql="SELECT country, city, street FROM addresses WHERE customerno=?">
            <input-parameter name="customerNo"/>
            <output-parameter name="addresses"/>
        </db-action>
        <db-action procedure="pkgAddress.sp_getAddresses(?, ?, ?, ?)">
            <input-parameter name="customerNo"/>
            <output-parameter name="country"/>
            <output-parameter name="city"/>
            <output-parameter name="street"/>
        </db-action>
        <db-action function="pkgAddress.fn_getAddresses(?)">
            <input-parameter name="customerNo"/>
            <output-parameter name="addresses"/>
        </db-action>
        <custom-action class="com.xx.GetAddressesAction">
            <input-parameter name="customerNo"/>
            <output-parameter name="addresses"/>
        </custom-action>
    </actions>
    <response>
        <addresses>
            <address/>
        </addresses>
    </response>
    <error/>
</service>